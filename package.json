      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "semi": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
